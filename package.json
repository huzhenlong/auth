{
    "name": "mfw-admin",
    "version": "0.3.2",
    "description": "mfw",
    "author": "mfw",
    "engines": {
        "node": ">= 4.2.1",
        "npm": ">= 3"
    },
    "license": "MIT",
    "scripts": {
        "precommit": "npm run lint-staged",
        "ng": "ng",
        "start": "ng serve -o",
        "start-jmz": "ng serve -o --proxy-config proxy.conf.js https://admin.jmz.dev",
        "start-hzl": "ng serve -p 4222 -o --proxy-config proxy.conf.js https://admin.huzhenlong.ab",
        "start-zdx": "ng serve -o --proxy-config proxy.conf.js https://admin.zdx.dev",
        "serve:hmr": "ng serve -o --hmr -e=hmr",
        "build": "rm -rf dist/; ng build -op dist/ --aot --prod -bh / --build-optimizer --deploy-url \"https://js.mafengwo.net/js/\"",
        "build-dev": "rm -rf dist/; ng build -op dist/ --deploy-url \"https://js.zdx.dev/js/guide-admin/\"",
        "analyze": "ng build --prod --build-optimizer --stats-json",
        "lint": "run-s lint:ts lint:style",
        "lint:ts": "ng lint",
        "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
        "lint-staged": "lint-staged",
        "e2e": "ng e2e",
        "test": "ng test -sr",
        "test-coverage": "ng test -sr -cc"
    },
